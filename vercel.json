{
  "builds": [
    {
      "src": "build.sh",
      "use": "@vercel/static-build"
    },
    {
      "src": "run.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ]
}
