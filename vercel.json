{
  "build": {
    "env": {
      "FLASK_ENV": "production"
    }
  },
  "builds": [
    {
      "src": "build.sh",
      "use": "@vercel/static-build"
    },
    {
      "src": "run.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ]
}